{
  "name": "my-sites",
  "description": "Monorepo of my personal sites",
  "keywords": [
    "portfolio",
    "blog",
    "music",
    "photography"
  ],
  "scripts": {
    "start-about": "cd ./about && npm start && cd -",
    "start-music": "cd ./music && npm start && cd -",
    "start-photos": "cd ./photos && npm start && cd -",
    "start-writing": "cd ./writing && npm run start && cd -",
    "start-uploader": "cd ./uploader && npm run start && cd -",
    "lint-about": "cd ./about && npm run lint && cd -",
    "lint-music": "cd ./music && npm run lint && cd -",
    "lint-photos": "cd ./photos && npm run lint && cd -",
    "lint-writing": "cd ./writing && npm run lint && cd -",
    "lint-uploader": "cd ./uploader && npm run lint && cd -",
    "lint": "npm run lint-about && npm run lint-music && npm run lint-photos && npm run lint-writing && npm run lint-uploader"
  },
  "author": "Evan Bonsignori",
  "license": "ISC",
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.4",
    "babel-eslint": "^10.1.0",
    "eslint": "7.23",
    "eslint-config-next": "^12.0.4",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-i18n-text": "^1.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2"
  },
  "dependencies": {
    "next": "^12.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "spinners-react": "^1.0.6",
    "styled-components": "^5.3.3"
  }
}
